		int[] res = allindices(arr, data, 0, 0);
		System.out.println(res[indx]);
	}

	public static int[] allindices(int[] arr, int data, int vidx, int cnt) {
		if (vidx == arr.length) {
			return new int[cnt];
		}
		if (data == arr[vidx]) {
			cnt++;
		}
		int[] result = allindices(arr, data, vidx + 1, cnt);
		if (data == arr[vidx]) {
			result[cnt - 1] = vidx;
		}

		return result;

	}
}
