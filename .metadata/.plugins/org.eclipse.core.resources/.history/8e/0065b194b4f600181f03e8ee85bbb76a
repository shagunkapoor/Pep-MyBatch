			int y=scn.nextInt();
			int res=matrixcolourings(c,s,n,x,y);
			System.out.println(res);
		}
	}
	
	private static int colouringspossible(String s, int n) {
		HashMap<Character, Integer> hmap=new HashMap<>();
		
		for(int i=0;i<n;i++){
			char ch=s.charAt(i);
			hmap.put(ch,1);
		}
		return 0;
	}

