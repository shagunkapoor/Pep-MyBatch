	public static int solve(int m, int ll, int ul, PriorityQueue pq) {
		int sum = ll;
		while (pq.size() > 0) {
			int rem = (int) pq.remove();

			if (pq.size() != 0) {
				int top = (int) pq.peek();
				sum += (top - 1) + (rem + 1);
			}
		}

		sum += ul;

		return sum;
	}
	
