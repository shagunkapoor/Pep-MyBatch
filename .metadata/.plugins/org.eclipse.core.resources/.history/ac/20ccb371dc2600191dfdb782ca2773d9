package practice;

public class Amazonques {

	private static final Exception ArrayIndexOutOfBoundsException = null;

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double arr[]={7.5,24.5,32.26};
		double max=32;
		int s=solve(arr, max);
		System.out.println(s);
	}
	
	public static int solve(double[] arr, double max){
		
		try{
			
			if(arr.length<1)
			throw ArrayIndexOutOfBoundsException;
			else
				return -1;
		}catch(Exception e){
			return 1;
		}
	}

}
