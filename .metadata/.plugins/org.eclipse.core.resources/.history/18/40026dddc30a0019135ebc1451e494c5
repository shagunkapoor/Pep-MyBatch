	public static int[] pitable(String pat){
		int[] arr=new int[pat.length()+1];
		
		Arrays.fill(arr, 0);
		
		int length=0;
		for(int i=0;i<pat.length();){
			char ch=pat.charAt(i);
			if(ch==pat.charAt(length)){
				
			}
		}
		
		for(char ch: pat.toCharArray()){
			
		}
			
		return arr;
		
	}

}
