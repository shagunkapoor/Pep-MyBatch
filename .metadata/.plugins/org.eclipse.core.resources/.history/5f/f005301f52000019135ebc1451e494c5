package practice;

import java.util.Collections;
import java.util.PriorityQueue;

public class ThoughtWorks {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n=5,k=3;
		int[] arr={1,4,10,8,15};
		mag(n,k,arr);

	}
	
	public static void mag(int n, int k, int[] arr){
		
		PriorityQueue<Integer> pq=new PriorityQueue<>(Collections.reverseOrder());
		
		for(int i=1;i<=k;i++){
			pq.add(Integer.MAX_VALUE);
		}
		
		int diff=0;
		int res=0;
		for(int x=0;x<n;x++){
			
			System.out.println(pq.peek());
			if(arr[x]==1){
				diff=3;
			}else{
				int pn1=(int)Math.sqrt(arr[x]);
				int pn2=(int) Math.round(Math.sqrt(arr[x]));
				diff=Math.min((pn2*pn2)-arr[x],arr[x]-(pn1*pn1));
				
			}
			
			if(diff<pq.peek()){
				pq.remove();
				pq.add(diff);
			}
			
			
			
			
		}
		
		while(pq.size()>0){
//			System.out.println(pq.peek());
			res+=pq.remove();
			
		}
		
		System.out.println("ans"+res);
	}

}

